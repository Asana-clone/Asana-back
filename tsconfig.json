{
  "compilerOptions": {
    "target": "ES2021",
    "skipLibCheck": true,
    "module": "commonjs",
    "strict":true,
    "strictNullChecks": false,
    "esModuleInterop": false,
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators":true,
    "outDir": "dist",
    "rootDir": "src"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}